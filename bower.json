{
  "name": "egrid",
  "version": "0.0.0",
  "authors": [
    "Yosuke ONOUE <onoue@likr-lab.com>"
  ],
  "license": "MIT",
  "private": true,
  "ignore": [
    "**/.*",
    "node_modules",
    "bower_components",
    "test",
    "tests"
  ],
  "dependencies": {
    "jquery": "~2.1.0",
    "bootstrap": "~3.2.0",
    "angular": "1.2",
    "d3": "~3.4.13",
    "angular-translate": "~2.6.0",
    "angular-translate-loader-static-files": "~2.0.0",
    "angular-ui-bootstrap-bower": "0.10",
    "angular-ui-router": "~0.2.11",
    "egrid-core": "~1.2.2",
    "nvd3": "~1.1.15-beta",
    "angular-dragdrop": "~1.0.8",
    "encoding.js": "https://github.com/polygonplanet/encoding.js.git#~1.0.9",
    "d3-downloadable": "*",
    "d3-pca": "*",
    "d3-cloud": "~1.0.5",
    "kuromoji": "~0.0.2"
  },
  "exportsOverride": {
    "angular-dragdrop": {
      "js": "src/*.js"
    },
    "angular-ui-router": {
      "js": "release/*.js"
    },
    "bootstrap": {
      "js": "dist/js/*.js",
      "css": "dist/css/*.css",
      "fonts": "dist/fonts/*"
    },
    "d3-cloud": {
      "js": "d3.layout.cloud.js"
    },
    "dagre": {
      "js": "dist/*.js"
    },
    "encoding.js": {
      "js": "encoding.js"
    },
    "graphlib": {
      "js": "dist/*.js"
    },
    "jquery": {
      "js": "dist/*.js",
      "map": "dist/*.map"
    },
    "jquery-ui": {
      "js": "jquery-ui.*"
    },
    "kuromoji": {
      "js": "**/*.js",
      "dict": "dist/dict/*"
    },
    "lodash": {
      "js": "dist/*.js"
    },
    "numericjs": {
      "js": "src/numeric.js"
    },
    "nvd3": {
      "js": "nv*.js",
      "css": "nv*.css"
    },
    "*": {
      "js": "**/*.js",
      "css": "**/*.css",
      "map": "**/*.map"
    }
  },
  "resolutions": {
    "d3": "~3.5.2"
  }
}
