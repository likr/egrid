{
  "name": "egrid",
  "version": "0.0.0",
  "authors": [
    "Yosuke ONOUE <onoue@likr-lab.com>"
  ],
  "license": "MIT",
  "private": true,
  "ignore": [
    "**/.*",
    "node_modules",
    "bower_components",
    "test",
    "tests"
  ],
  "dependencies": {
    "angular": "1.3",
    "angular-dragdrop": "1.0",
    "angular-new-router": "0.5",
    "angular-translate": "2.6",
    "angular-translate-loader-static-files": "2.6",
    "angular-ui-bootstrap-bower": "0.12",
    "bootstrap": "~3.2.0",
    "d3": "~3.4.13",
    "d3-cloud": "~1.0.5",
    "d3-downloadable": "*",
    "d3-pca": "*",
    "egrid-client": "https://github.com/likr/egrid-client.git",
    "egrid-core": "~1.2.2",
    "encoding.js": "https://github.com/polygonplanet/encoding.js.git#~1.0.9",
    "jquery": "~2.1.0",
    "kuromoji": "~0.0.2",
    "nvd3": "~1.1.15-beta",
    "semjs": "https://github.com/likr/semjs.git"
  },
  "exportsOverride": {
    "angular-dragdrop": {
      "js": "src/*.js"
    },
    "angular-new-router": {
      "js": "dist/router.*.js"
    },
    "bootstrap": {
      "js": "dist/js/*.js",
      "css": "dist/css/*.css",
      "fonts": "dist/fonts/*"
    },
    "d3-cloud": {
      "js": "d3.layout.cloud.js"
    },
    "dagre": {
      "js": "dist/*.js"
    },
    "encoding.js": {
      "js": "encoding.js"
    },
    "graphlib": {
      "js": "dist/*.js"
    },
    "jquery": {
      "js": "dist/*.js",
      "map": "dist/*.map"
    },
    "jquery-ui": {
      "js": "jquery-ui.*"
    },
    "kuromoji": {
      "js": "**/*.js",
      "dict": "dist/dict/*"
    },
    "lodash": {
      "js": "dist/*.js"
    },
    "numericjs": {
      "js": "src/numeric.js"
    },
    "nvd3": {
      "js": "nv*.js",
      "css": "nv*.css"
    },
    "*": {
      "js": "**/*.js",
      "css": "**/*.css",
      "map": "**/*.map"
    }
  },
  "resolutions": {
    "angular": "1.3",
    "d3": "~3.5.2"
  }
}
